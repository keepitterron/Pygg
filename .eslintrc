{
  "extends": [
    "./node_modules/@usabilla/js-styleguide/eslint-config-usabilla/index.js"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  },
  "plugins": [
    "react"
  ]
}
